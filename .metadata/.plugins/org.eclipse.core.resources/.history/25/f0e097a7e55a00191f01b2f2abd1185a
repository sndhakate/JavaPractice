package com.practicePrograms;

public class FibonacciUsingWhileLoop {
	
	public static void main(String[] args) {
		
		int n, a=0, b=0, c=1;
		System.out.println("Fibonacci Series of" +n+" numbers:");
		
		int i=1;
		while (i<=n) {
			System.out.print(a+"  ");
			int sumOfPrevTwo = a+b;
			a=b;
			b=sumOfPrevTwo;
			i++;
		}
		
	}

}
