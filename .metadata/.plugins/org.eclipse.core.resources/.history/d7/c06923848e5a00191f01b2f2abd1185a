package com.demo;

import java.io.File;

public class CreateNewFile {
	public static void main(String[] args) {
		
		File f = new File("C:/");
		
		boolean fvar = f.createNewFile();
		
		if (fvar) {
			System.out.println("File is successfully created");
		} else {
			
			System.out.println("File is already present");
		}
	}

}
