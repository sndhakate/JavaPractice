package com.practicePrograms;

import java.util.Scanner;

public class TP {
	


    public static void main (String[] args) {
        int n, a=0, b=0, c=1;
        System.out.println("enter the number of n:");
        
        Scanner s = new Scanner(System.in);
        n=s.nextInt();
        
        System.out.println("Fibonacci series :");
        for(int i=1;i<=n;i++){
            a=b;
            b=c;
            c=a+b;
            System.out.print(a+ " ");
        }
    }
}


